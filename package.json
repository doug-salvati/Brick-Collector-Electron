{
  "name": "brick-collector",
  "version": "0.1.1",
  "main": "main.js",
  "description": "App for managing a brick collection.",
  "repository": "https://github.com/doug-salvati/Brick-Collector",
  "author": "Doug Salvati",
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
