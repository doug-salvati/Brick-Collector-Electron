    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "css-loader": "^3.1.0",
    "electron": "^7.1.7",
    "electron-builder": "^21.2.0",
    "electron-packager": "^12.1.2",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "file-loader": "^1.1.11",
    "jest": "^24.8.0",
    "node-libs-browser": "^2.1.0",
    "style-loader": "^0.21.0",
    "webpack": "^4.20.2",
